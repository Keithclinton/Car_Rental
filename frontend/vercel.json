{
  "buildCommand": "npx pnpm install --no-frozen-lockfile && npx pnpm run build",
  "outputDirectory": "dist",
  "devCommand": "pnpm run dev",
  "installCommand": "npx pnpm install --no-frozen-lockfile",
  "framework": "vite",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
